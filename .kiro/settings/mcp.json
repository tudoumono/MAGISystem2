{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}",
        "PATH": "C:\\fnm\\data\\node-versions\\v22.12.0\\installation;${PATH}"
      },
      "disabled": false,
      "autoApprove": [
        "search_repositories",
        "get_file_contents",
        "list_files",
        "get_repository_info"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "."
      ],
      "env": {
        "PATH": "C:\\fnm\\data\\node-versions\\v22.12.0\\installation;${PATH}"
      },
      "disabled": true,
      "autoApprove": [
        "read_file",
        "list_directory",
        "search_files",
        "create_directory"
      ]
    },
    "web-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "${BRAVE_API_KEY}",
        "PATH": "C:\\fnm\\data\\node-versions\\v22.12.0\\installation;${PATH}"
      },
      "disabled": false,
      "autoApprove": [
        "search",
        "get_page_content",
        "brave_web_search"
      ],
      "disabledTools": [
        "brave_web_search",
        "brave_local_search"
      ]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ],
      "env": {
        "PATH": "C:\\fnm\\data\\node-versions\\v22.12.0\\installation;${PATH}"
      },
      "disabled": false,
      "autoApprove": [
        "performance_start_trace",
        "list_pages",
        "navigate_page",
        "take_snapshot",
        "fill",
        "list_console_messages",
        "click",
        "wait_for",
        "evaluate_script",
        "list_network_requests",
        "close_page",
        "new_page"
      ]
    }
  }
}
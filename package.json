{
  "name": "magi-decision-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "setup-env": "node scripts/setup-env.js",
    "validate-env": "node scripts/setup-env.js validate",
    "check:amplify": "node scripts/check-amplify-setup.js",
    "setup:amplify": "node scripts/setup-amplify-environment.js",
    "amplify:sandbox": "npx ampx sandbox",
    "amplify:deploy": "npx ampx sandbox --once",
    "amplify:sso-login": "aws sso login",
    "amplify:deploy-sso": "aws sso login && npx ampx sandbox --once",
    "amplify:status": "npx ampx status",
    "amplify:logs": "npx ampx logs",
    "amplify:generate": "npx ampx generate graphql-client-code --format modelgen --model-target typescript"
  },
  "dependencies": {
    "@aws-amplify/adapter-nextjs": "^1.6.10",
    "@aws-amplify/backend": "^1.6.0",
    "@aws-amplify/backend-cli": "^1.4.0",
    "@aws-amplify/ui-react": "^6.5.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.918.0",
    "@aws-sdk/client-cloudwatch": "^3.705.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.705.0",
    "@aws-sdk/client-lambda": "^3.918.0",
    "@opentelemetry/auto-instrumentations-node": "^0.66.0",
    "@opentelemetry/exporter-otlp-http": "^0.26.0",
    "@opentelemetry/id-generator-aws-xray": "^2.0.3",
    "@opentelemetry/propagator-aws-xray": "^2.1.3",
    "@opentelemetry/sdk-node": "^0.207.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "aws-amplify": "^6.8.0",
    "aws-xray-sdk-core": "^3.9.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "next": "15.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.5.6",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
